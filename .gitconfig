[user]
	email = zach.fogg@gmail.com
	name = Zach Fogg

[alias]
    gh = !sh -c 'git clone git@github.com:$1' -

[push]
	default = upstream

[core]
	editor = vim
	pager = vimpager
	excludesfile = ~/.gitignore_global
    eol = lf
	autocrlf = input

[diff]
	tool = vimdiff

[merge]
	tool = vimdiff

[color]
	ui = true
