Host *
    # Forward ssh agent to the remote machine.
    ForwardAgent yes
    # Automatically add hosts on connect.
    StrictHostKeyChecking no

Host zf
    Hostname zachfogg.com
    IdentityFile ~/.ssh/id_rsa
    User zachfogg

Host dorm
    Hostname zfogg.student.umd.edu
    IdentityFile ~/.ssh/id_rsa
    user zach

Host gh
    Hostname github.com
    IdentityFile ~/.ssh/id_rsa
    user git
